   "source": [
    "with open(\"seq_ex.fasta\", \"r\") as file:\n",
    "    seq = file.readline()\n",
    "    print(seq)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "8d1f97af-b654-41b9-b356-c7b2727cb7ad",
   "metadata": {},
   "outputs": [],
   "source": [
    "gc_percent = 0\n",
    "for bp in seq:\n",
    "    if bp == 'G' or bp == 'C':\n",
    "        gc_percent += 1\n",
    "gc_percent = (gc_percent/len(seq)) * 100\n",
    "print(gc_percent)"
   ]
  }
